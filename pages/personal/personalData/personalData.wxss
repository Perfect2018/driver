/* pages/personal/personalData/personalData.wxss */
page{
  background-color: #ececec;
  padding: 20rpx;
  box-sizing: border-box;
}

.personal{
  background-color: #fff;
  padding: 10rpx 40rpx 20rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

.personal-item{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid #ccc;
  font-size: 30rpx;
}

.personal-item>view{
  display: flex;
  align-items: center;
}

.icon-right{
  margin-left: 20rpx;
}

.rate{
  color: #F4EA2A;
}